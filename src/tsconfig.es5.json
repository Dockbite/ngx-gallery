{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es5",
    "baseUrl": ".",
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "../build",
    "rootDir": ".",
    "lib": [
      "es2015",
      "dom"
    ],
  "paths": {
      "@dockbite/ngx-gallery": [
          "dist"
      ],
      "@dockbite/ngx-gallery/*": [
          "dist/*"
      ]
  },
    "skipLibCheck": true,
    "types": []
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "ngx-gallery.js",
    "flatModuleId": "@dockbite/ngx-gallery"
  },
  "files": [
    "./index.ts"
  ]
}
